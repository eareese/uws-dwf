doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(name='theme-color' content='#232364')
    title= htmlWebpackPlugin.options.title
    link(rel='icon' href='img/favicon.png')
    link(rel='manifest' href='manifest.json')
  body
    div(class='container')
      #row_svg(class='row')
        svg#ky_home(
          width='100%'
          height='100%'
          viewBox='0 0 662 662'
          preserveAspectRatio='xMinYMin meet'
        )
      #row_inputs(class='row')
        input#text_top(class='u-full-width' type='text' value='')
        input#text_btm(class='u-full-width' type='text' value='')
        a#get_img(class='button button-primary' href='#') Get image
      div(class='row')
        img#seal2img
